<iaapi:Project xmlns:iaapi="http://www.ibm.com/investigate/api/iaapi" name="CocoPharma">
    <DataSources>
        <DataSource host="DB.INFOSVR.VAGRANT.IBM.COM" name="LOCATION">
            <Schema name="{{ egeria_samples_db_schema }}">
                <Table name="WORKLOCATION">
                    <Column name="ADDR6"/>
                    <Column name="ADDR7"/>
                    <Column name="WLNAME"/>
                    <Column name="ADDR1"/>
                    <Column name="ADDR3"/>
                    <Column name="WLID"/>
                    <Column name="ADDR5"/>
                    <Column name="ADDR2"/>
                    <Column name="ADDR4"/>
                </Table>
            </Schema>
        </DataSource>
        <DataSource host="DB.INFOSVR.VAGRANT.IBM.COM" name="EMPLOYEE">
            <Schema name="{{ egeria_samples_db_schema }}">
                <Table name="DEPT">
                    <Column name="DEPNAME"/>
                    <Column name="MANAGER"/>
                    <Column name="DEPCODE"/>
                </Table>
                <Table name="EMPLOYEE">
                    <Column name="DEPT"/>
                    <Column name="ROLE"/>
                    <Column name="LOCCODE"/>
                    <Column name="EMPSTATUS"/>
                    <Column name="LNAME"/>
                    <Column name="LVL"/>
                    <Column name="FNAME"/>
                    <Column name="PNUM"/>
                </Table>
            </Schema>
        </DataSource>
        <DataSource host="DB.INFOSVR.VAGRANT.IBM.COM" name="COMPDIR">
            <Schema name="{{ egeria_samples_db_schema }}">
                <Table name="CONTACTLIST">
                    <Column name="JOBTITLE"/>
                    <Column name="LASTNAME"/>
                    <Column name="WORKLOCATION"/>
                    <Column name="COMPANY"/>
                    <Column name="FIRSTNAME"/>
                    <Column name="RECID"/>
                    <Column name="CONTACTTYPE"/>
                </Table>
                <Table name="CONTACTPHONE">
                    <Column name="CONTACTTYPE"/>
                    <Column name="NUMBER"/>
                    <Column name="RECID"/>
                </Table>
                <Table name="CONTACTEMAIL">
                    <Column name="REDIF"/>
                    <Column name="ETYPE"/>
                    <Column name="EMAIL"/>
                </Table>
            </Schema>
        </DataSource>
        <DataSource host="DB.INFOSVR.VAGRANT.IBM.COM" name="EMPLSANL">
            <Schema name="{{ egeria_samples_db_schema }}">
                <Table name="EMPSALARYANALYSIS">
                    <Column name="ETYPE"/>
                    <Column name="STATE"/>
                    <Column name="HOL"/>
                    <Column name="EMPSTATUS"/>
                    <Column name="SERVICE"/>
                    <Column name="PNUM"/>
                    <Column name="FNAME"/>
                    <Column name="HDR"/>
                    <Column name="LNAME"/>
                    <Column name="LOCCODE"/>
                    <Column name="SNUM"/>
                    <Column name="DEPT"/>
                    <Column name="BONUS"/>
                    <Column name="RECTYPE"/>
                    <Column name="TAXP"/>
                    <Column name="LOCATION"/>
                    <Column name="ROLE"/>
                    <Column name="LVL"/>
                    <Column name="STREET"/>
                    <Column name="CITY"/>
                    <Column name="SALARY"/>
                    <Column name="TAX"/>
                </Table>
            </Schema>
        </DataSource>
        <DataSource host="DB.INFOSVR.VAGRANT.IBM.COM" name="PATIENT">
            <Schema name="{{ egeria_samples_db_schema }}">
                <Table name="PATIENT">
                    <Column name="LH_ADDR_3"/>
                    <Column name="TRIAL"/>
                    <Column name="LH_ADDR_5"/>
                    <Column name="LH_ADDR_2"/>
                    <Column name="LH_ADDR_4"/>
                    <Column name="PATIENTID"/>
                    <Column name="PATIENT"/>
                    <Column name="LH_ADDR_1"/>
                </Table>
            </Schema>
        </DataSource>
    </DataSources>
    <DataRuleDefinitions>
        <DataRuleDefinition folder="All" name="r_00001">
            <description>Checks that department code is valid</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="any" name="dept_code"/>
                <Variable dataType="any" name="dept_code_list"/>
            </Variables>
            <ExecutableRules>
                <ExecutableRule folder="All" name="rx_00001" excludeDuplicates="false">
                    <description/>
                    <LongDescription/>
                    <BoundExpression>DB.INFOSVR.VAGRANT.IBM.COM.EMPLOYEE.{{ egeria_samples_db_schema }}."EMPLOYEE".DEPT in_reference_column DB.INFOSVR.VAGRANT.IBM.COM.EMPLOYEE.{{ egeria_samples_db_schema }}."DEPT".DEPCODE</BoundExpression>
                    <OutputDefinition type="FailingRecords">
                        <OutputColumn name="dept_code" type="variable" value="dept_code"/>
                        <OutputColumn name="PNUM" type="column" value="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.EMPLOYEE.PNUM"/>
                    </OutputDefinition>
                    <Bindings>
                        <Binding var="dept_code_list">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.DEPT.DEPCODE"/>
                        </Binding>
                        <Binding var="dept_code">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.EMPLOYEE.DEPT"/>
                        </Binding>
                    </Bindings>
                </ExecutableRule>
            </ExecutableRules>
            <expression>dept_code in_reference_column  dept_code_list</expression>
        </DataRuleDefinition>
        <DataRuleDefinition folder="All" name="v_00001">
            <description>Checks that contact type is valid</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="any" name="contact_type"/>
            </Variables>
            <ExecutableRules>
                <ExecutableRule folder="All" name="vx_00001_CONTACTTYPE" excludeDuplicates="false">
                    <description/>
                    <LongDescription/>
                    <BoundExpression>DB.INFOSVR.VAGRANT.IBM.COM.COMPDIR.{{ egeria_samples_db_schema }}."CONTACTLIST".CONTACTTYPE in_reference_list   {'E','P','C'}</BoundExpression>
                    <OutputDefinition type="FailingRecords">
                        <OutputColumn name="contact_type" type="variable" value="contact_type"/>
                        <OutputColumn name="RECID" type="column" value="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.COMPDIR.{{ egeria_samples_db_schema }}.CONTACTLIST.RECID"/>
                    </OutputDefinition>
                    <Bindings>
                        <Binding var="contact_type">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.COMPDIR.{{ egeria_samples_db_schema }}.CONTACTLIST.CONTACTTYPE"/>
                        </Binding>
                    </Bindings>
                </ExecutableRule>
            </ExecutableRules>
            <expression>contact_type in_reference_list  {'E','P','C'}</expression>
        </DataRuleDefinition>
        <DataRuleDefinition folder="All" name="v_00002">
            <description>Checks for valid email type</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="any" name="email_type"/>
            </Variables>
            <ExecutableRules>
                <ExecutableRule folder="All" name="vx_00002_CONTACTEMAIL" excludeDuplicates="false">
                    <description/>
                    <LongDescription/>
                    <BoundExpression>DB.INFOSVR.VAGRANT.IBM.COM.COMPDIR.{{ egeria_samples_db_schema }}."CONTACTEMAIL".ETYPE in_reference_list   {'S','C'}</BoundExpression>
                    <OutputDefinition type="FailingRecords">
                        <OutputColumn name="email_type" type="variable" value="email_type"/>
                        <OutputColumn name="REDIF" type="column" value="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.COMPDIR.{{ egeria_samples_db_schema }}.CONTACTEMAIL.REDIF"/>
                    </OutputDefinition>
                    <Bindings>
                        <Binding var="email_type">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.COMPDIR.{{ egeria_samples_db_schema }}.CONTACTEMAIL.ETYPE"/>
                        </Binding>
                    </Bindings>
                </ExecutableRule>
            </ExecutableRules>
            <expression>email_type in_reference_list  {'S','C'}</expression>
        </DataRuleDefinition>
        <DataRuleDefinition folder="All" name="v_00003">
            <description>Checks that employee level is valid</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="numeric" name="emp_level"/>
            </Variables>
            <expression>emp_level &lt;=10</expression>
        </DataRuleDefinition>
        <DataRuleDefinition folder="All" name="v_00004">
            <description>Checks that employee status is valid</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="numeric" name="emp_status"/>
            </Variables>
            <expression>emp_status &lt;=7</expression>
        </DataRuleDefinition>
        <RuleSetDefinition folder="All" name="vs_00001" separateVariablesByRule="false">
            <description>Checks that a salary analysis record is valid</description>
            <LongDescription/>
            <Variables>
                <Variable dataType="numeric" name="emp_level"/>
                <Variable dataType="numeric" name="emp_status"/>
            </Variables>
            <ExecutableRules>
                <ExecutableRule folder="All" name="vsx_00001_EMPLOYEE" excludeDuplicates="false">
                    <description/>
                    <LongDescription/>
                    <BoundExpression>DB.INFOSVR.VAGRANT.IBM.COM.EMPLOYEE.{{ egeria_samples_db_schema }}."EMPLOYEE".EMPSTATUS &lt;=7;
                        DB.INFOSVR.VAGRANT.IBM.COM.EMPLOYEE.{{ egeria_samples_db_schema }}."EMPLOYEE".LVL &lt;=10</BoundExpression>
                    <OutputDefinition type="FailingRecords">
                        <OutputColumn name="emp_level" type="variable" value="emp_level"/>
                        <OutputColumn name="emp_status" type="variable" value="emp_status"/>
                        <OutputColumn name="PNUM" type="column" value="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.EMPLOYEE.PNUM"/>
                    </OutputDefinition>
                    <Bindings>
                        <Binding var="emp_status">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.EMPLOYEE.EMPSTATUS"/>
                        </Binding>
                        <Binding var="emp_level">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLOYEE.{{ egeria_samples_db_schema }}.EMPLOYEE.LVL"/>
                        </Binding>
                    </Bindings>
                </ExecutableRule>
                <ExecutableRule folder="All" name="vsx_00001_EMPSALARYANALYSIS" excludeDuplicates="false">
                    <description/>
                    <LongDescription/>
                    <BoundExpression>DB.INFOSVR.VAGRANT.IBM.COM.EMPLSANL.{{ egeria_samples_db_schema }}."EMPSALARYANALYSIS".EMPSTATUS &lt;=7;
                        DB.INFOSVR.VAGRANT.IBM.COM.EMPLSANL.{{ egeria_samples_db_schema }}."EMPSALARYANALYSIS".LVL &lt;=10</BoundExpression>
                    <OutputDefinition type="FailingRecords">
                        <OutputColumn name="emp_level" type="variable" value="emp_level"/>
                        <OutputColumn name="emp_status" type="variable" value="emp_status"/>
                        <OutputColumn name="PNUM" type="column" value="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLSANL.{{ egeria_samples_db_schema }}.EMPSALARYANALYSIS.PNUM"/>
                    </OutputDefinition>
                    <Bindings>
                        <Binding var="emp_level">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLSANL.{{ egeria_samples_db_schema }}.EMPSALARYANALYSIS.LVL"/>
                        </Binding>
                        <Binding var="emp_status">
                            <Column name="&quot;DB.INFOSVR.VAGRANT.IBM.COM&quot;.EMPLSANL.{{ egeria_samples_db_schema }}.EMPSALARYANALYSIS.EMPSTATUS"/>
                        </Binding>
                    </Bindings>
                </ExecutableRule>
            </ExecutableRules>
            <RuleDefinitionReference ruleName="v_00004"/>
            <RuleDefinitionReference ruleName="v_00003"/>
        </RuleSetDefinition>
    </DataRuleDefinitions>
    <Folders>
        <Folder name="All">
            <description>Global category for all QualityComponents</description>
            <LongDescription>Global category for all QualityComponents</LongDescription>
        </Folder>
    </Folders>
</iaapi:Project>
